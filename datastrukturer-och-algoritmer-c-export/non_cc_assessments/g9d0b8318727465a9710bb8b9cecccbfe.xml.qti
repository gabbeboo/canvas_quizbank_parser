            <fieldentry>1473,8119,2125,1311,7580</fieldentry>
          </qtimetadatafield>
        </qtimetadata>
      </itemmetadata>
      <presentation>
        <material>
          <mattext texttype="text/html">&lt;div&gt;
&lt;p&gt;Antag att du har följande funktion:&lt;/p&gt;
&lt;p&gt;&lt;code&gt;def mult(n):&lt;br&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; if (n == 1):&lt;br&gt;&amp;nbsp; &amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp; return 1;&lt;br&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; else:&lt;br&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; return n * mult(n-1);&lt;/code&gt;&lt;/p&gt;
&lt;p&gt;Vad blir resultatet av att anropa funktionen med värdet 4?&lt;/p&gt;
&lt;/div&gt;</mattext>
        </material>
        <response_lid ident="response1" rcardinality="Single">
          <render_choice>
            <response_label ident="1473">
              <material>
                <mattext texttype="text/plain">4</mattext>
              </material>
            </response_label>
            <response_label ident="8119">
              <material>
                <mattext texttype="text/plain">12</mattext>
              </material>
            </response_label>
            <response_label ident="2125">
              <material>
                <mattext texttype="text/plain">24</mattext>
              </material>
            </response_label>
            <response_label ident="1311">
              <material>
                <mattext texttype="text/plain">48</mattext>
              </material>
            </response_label>
            <response_label ident="7580">
              <material>
                <mattext texttype="text/plain">1</mattext>
              </material>
            </response_label>
          </render_choice>
        </response_lid>
      </presentation>
      <resprocessing>
        <outcomes>
          <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
        </outcomes>
        <respcondition continue="No">
          <conditionvar>
            <varequal respident="response1">2125</varequal>
          </conditionvar>
          <setvar action="Set" varname="SCORE">100</setvar>
        </respcondition>
      </resprocessing>
    </item>
    <item ident="gc90d7383fcddfa7a7800d9603c924230" title="Multiple Choice">
