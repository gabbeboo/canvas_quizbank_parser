      <presentation>
        <material>
          <mattext texttype="text/html">&lt;div&gt;&lt;br&gt;Vilket påstående är falskt när det gäller rekursion?&lt;/div&gt;</mattext>
        </material>
        <response_lid ident="response1" rcardinality="Single">
          <render_choice>
            <response_label ident="6693">
              <material>
                <mattext texttype="text/plain">Rekursiva funktioner kan skrivas om till icke-rekursiva funktioner.</mattext>
              </material>
            </response_label>
            <response_label ident="7817">
              <material>
                <mattext texttype="text/plain">Rekursiva funktioner tar normalt upp mer minne än motsvarande icke-rekursiva funktioner.</mattext>
              </material>
            </response_label>
            <response_label ident="5603">
              <material>
                <mattext texttype="text/plain">Rekursiva funktioner går normalt snabbare än motsvarande icke-rekursiva funktioner.</mattext>
              </material>
            </response_label>
            <response_label ident="8799">
              <material>
                <mattext texttype="text/plain">Rekursion gör programmen lättare att läsa och förstå.</mattext>
              </material>
            </response_label>
          </render_choice>
        </response_lid>
      </presentation>
      <resprocessing>
        <outcomes>
          <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
        </outcomes>
        <respcondition continue="No">
          <conditionvar>
            <varequal respident="response1">5603</varequal>
          </conditionvar>
          <setvar action="Set" varname="SCORE">100</setvar>
        </respcondition>
      </resprocessing>
    </item>
    <item ident="g2730da6a9e12cb66cb6735742553eef8" title="Multiple Choice">
      <itemmetadata>
        <qtimetadata>
          <qtimetadatafield>
            <fieldlabel>question_type</fieldlabel>
            <fieldentry>multiple_choice_question</fieldentry>
          </qtimetadatafield>
          <qtimetadatafield>
            <fieldlabel>points_possible</fieldlabel>
            <fieldentry>1.0</fieldentry>
          </qtimetadatafield>
          <qtimetadatafield>
            <fieldlabel>original_answer_ids</fieldlabel>
            <fieldentry>9825,5381,5316,4079</fieldentry>
          </qtimetadatafield>
        </qtimetadata>
      </itemmetadata>
      <presentation>
        <material>
          <mattext texttype="text/html">&lt;div&gt;
&lt;p&gt;Nedanstående är pseduokod för att beräkna fakulteten för ett tal. Vad ska stå på sista raden?&lt;/p&gt;
&lt;p&gt;&lt;span style="font-family: Courier New , Courier , monospace;"&gt;Algorithm fact(num)&lt;br&gt;&amp;nbsp; &amp;nbsp; if num = 0 then&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; return 1&lt;br&gt;&amp;nbsp; &amp;nbsp; else&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; return ____________________&lt;/span&gt;&lt;/p&gt;
&lt;/div&gt;</mattext>
        </material>
        <response_lid ident="response1" rcardinality="Single">
          <render_choice>
            <response_label ident="9825">
              <material>
                <mattext texttype="text/html">&lt;span style="font-family: Courier New , Courier , monospace;"&gt;num*fact(num-1)&lt;/span&gt;</mattext>
              </material>
            </response_label>
            <response_label ident="5381">
              <material>
                <mattext texttype="text/html">&lt;span style="font-family: Courier New , Courier , monospace;"&gt;(num-1)*(num-2)&lt;/span&gt;</mattext>
              </material>
            </response_label>
            <response_label ident="5316">
              <material>
                <mattext texttype="text/html">&lt;span style="font-family: Courier New , Courier , monospace;"&gt;num*(num-1)&lt;/span&gt;</mattext>
              </material>
            </response_label>
            <response_label ident="4079">
              <material>
                <mattext texttype="text/html">&lt;span style="font-family: Courier New , Courier , monospace;"&gt;fact(num)*fact(num-1)&lt;/span&gt;</mattext>
              </material>
            </response_label>
          </render_choice>
        </response_lid>
      </presentation>
      <resprocessing>
        <outcomes>
          <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
        </outcomes>
        <respcondition continue="No">
          <conditionvar>
            <varequal respident="response1">9825</varequal>
          </conditionvar>
          <setvar action="Set" varname="SCORE">100</setvar>
        </respcondition>
      </resprocessing>
    </item>
