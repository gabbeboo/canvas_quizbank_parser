              </material>
            </response_label>
            <response_label ident="5309">
              <material>
                <mattext texttype="text/plain">7</mattext>
              </material>
            </response_label>
            <response_label ident="9977">
              <material>
                <mattext texttype="text/plain">Oändlig loop</mattext>
              </material>
            </response_label>
            <response_label ident="4547">
              <material>
                <mattext texttype="text/plain">17</mattext>
              </material>
            </response_label>
          </render_choice>
        </response_lid>
