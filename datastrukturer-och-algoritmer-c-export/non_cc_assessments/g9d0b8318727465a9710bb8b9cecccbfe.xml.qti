    if i=0 then
        return j
    else
        return test(i-1, i+j)&lt;/pre&gt;
&lt;p&gt;Vad blir resultatet av &lt;span style="font-family: Courier New , Courier , monospace;"&gt;test(4, 7)&lt;/span&gt;?&lt;/p&gt;
&lt;/div&gt;</mattext>
