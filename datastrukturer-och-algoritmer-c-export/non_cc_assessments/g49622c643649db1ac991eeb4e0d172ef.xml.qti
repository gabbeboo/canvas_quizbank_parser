      <presentation>
        <material>
          <mattext texttype="text/html">&lt;div&gt;&lt;br&gt;En instans av datatypen relation består av alla objekt som har samma [svar]. Ett exempel kan vara relationen &amp;lt; (mindre än) vars instans innehåller alla talpar där&amp;lt; är sann dvs (1,2), (1,3), (2,7), (1,9), ....&lt;/div&gt;</mattext>
        </material>
        <response_str ident="response1" rcardinality="Single">
          <render_fib>
            <response_label ident="answer1" rshuffle="No"/>
          </render_fib>
        </response_str>
      </presentation>
      <resprocessing>
        <outcomes>
          <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
        </outcomes>
        <respcondition continue="No">
          <conditionvar>
            <varequal respident="response1">egenskap</varequal>
            <varequal respident="response1">egenskaper</varequal>
            <varequal respident="response1">typ</varequal>
          </conditionvar>
          <setvar action="Set" varname="SCORE">100</setvar>
        </respcondition>
      </resprocessing>
