        <response_lid ident="response1" rcardinality="Multiple">
          <render_choice>
            <response_label ident="6663">
              <material>
                <mattext texttype="text/plain">Ett objekt instansieras från en klass och man säger att ett objekt är en instans av en klass.</mattext>
              </material>
