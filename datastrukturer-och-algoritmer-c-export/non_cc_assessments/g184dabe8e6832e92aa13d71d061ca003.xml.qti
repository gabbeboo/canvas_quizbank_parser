    <item ident="g7ee02f566da80b84a0ce172e72ab1f08" title="Klasser och objekt">
      <itemmetadata>
        <qtimetadata>
          <qtimetadatafield>
            <fieldlabel>question_type</fieldlabel>
            <fieldentry>multiple_choice_question</fieldentry>
          </qtimetadatafield>
          <qtimetadatafield>
            <fieldlabel>points_possible</fieldlabel>
            <fieldentry>1.0</fieldentry>
          </qtimetadatafield>
          <qtimetadatafield>
            <fieldlabel>original_answer_ids</fieldlabel>
            <fieldentry>8437,6257,9691,6147</fieldentry>
          </qtimetadatafield>
        </qtimetadata>
      </itemmetadata>
      <presentation>
        <material>
          <mattext texttype="text/html">&lt;div&gt;
&lt;p&gt;Vad händer när följande programkod körs? (Fundera först innan du provkör!)&lt;/p&gt;
&lt;pre&gt;&lt;span style="font-family: Courier New , Courier , monospace;"&gt;class test:
     def __init__(self,txt="Hello World"):
         self.txt = txt
&amp;nbsp;
     def display(self):
         print(self.txt)

obj=test()
obj.display()&lt;/span&gt;&lt;/pre&gt;
&lt;/div&gt;</mattext>
        </material>
        <response_lid ident="response1" rcardinality="Single">
          <render_choice>
            <response_label ident="8437">
              <material>
                <mattext texttype="text/plain">Programmet är felaktigt, konstruktorer kan inte ha default-parametrar.</mattext>
              </material>
            </response_label>
            <response_label ident="6257">
              <material>
                <mattext texttype="text/plain">Inget skrivs ut.</mattext>
              </material>
            </response_label>
            <response_label ident="9691">
              <material>
                <mattext texttype="text/plain">"Hello World" skrivs ut på skärmen.</mattext>
              </material>
            </response_label>
            <response_label ident="6147">
              <material>
                <mattext texttype="text/plain">Programmet är felaktigt, det saknas argument när display-funktionen anropas.</mattext>
              </material>
            </response_label>
          </render_choice>
        </response_lid>
      </presentation>
      <resprocessing>
        <outcomes>
          <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
        </outcomes>
        <respcondition continue="No">
          <conditionvar>
            <varequal respident="response1">9691</varequal>
          </conditionvar>
          <setvar action="Set" varname="SCORE">100</setvar>
        </respcondition>
      </resprocessing>
    </item>
    <item ident="g7993cc33e97ed182d4c6889ebbffc672" title="Klasser och objekt">
